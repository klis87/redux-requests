{
  "name": "redux-saga-requests-promise",
  "version": "0.1.0",
  "description": "Promise driver for Redux-Saga-Requests, Redux-Saga addon to simplify AJAX requests",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/redux-saga-requests-promise-driver.min.js",
  "repository": "git@github.com:klis87/redux-saga-requests.git",
  "author": "Konrad Lisiczynski <klisiczynski@gmail.com>",
  "license": "MIT",
  "typings": "types/index.d.ts",
  "keywords": [
    "redux",
    "redux-saga",
    "promise",
    "fetch",
    "ajax"
  ],
  "homepage": "https://github.com/klis87/redux-saga-requests",
  "bugs": {
    "url": "https://github.com/klis87/redux-saga-requests/issues"
  },
  "scripts": {
    "clean": "rimraf es lib dist",
    "lint": "eslint 'src/**'",
    "test": "jest src",
    "test:cover": "jest --coverage src",
    "test-types": "tsc types/index.d.spec.ts --noEmit --lib es2015,DOM",
    "build:commonjs": "cross-env BABEL_ENV=test babel src --out-dir lib --ignore 'src/**/*.spec.js'",
    "build:es": "cross-env babel src --out-dir es --ignore 'src/**/*.spec.js'",
    "build:umd": "webpack --mode development -o dist/redux-saga-requests-promise.js",
    "build:umd:min": "webpack --mode production -o dist/redux-saga-requests-promise.min.js",
    "build": "npm-run-all clean -p build:commonjs build:es build:umd build:umd:min",
    "build:watch": "nodemon --watch src --ignore src/**/*.spec.js --exec 'yarn run build:es'",
    "prepublish": "yarn run build"
  },
  "peerDependencies": {
    "redux-saga-requests": ">=0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "cross-env": "7.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "jest": "25.1.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11"
  }
}
